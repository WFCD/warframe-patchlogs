{
  "name": "warframe-patchlogs",
  "version": "0.0.0",
  "description": "All warframe patchlogs parsed to more usable JSON. Also lets you find all patchlogs for a specific item.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/WFCD/warframe-patchlogs.git"
  },
  "author": "Kaptard",
  "license": "MIT",
  "contributors": [
    "Tobiah <tobiah@pm.me>"
  ],
  "bugs": {
    "url": "https://github.com/WFCD/warframe-patchlogs/issues"
  },
  "scripts": {
    "build": "node build/update.js",
    "lint": "eslint .",
    "test": "nyc mocha",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "npm_config_yes=true npx clean-package"
  },
  "homepage": "https://github.com/WFCD/warframe-patchlogs#readme",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.3",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-private-methods": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@types/cheerio": "^0.22.30",
    "@types/progress": "^2.0.5",
    "cheerio": "^1.0.0-rc.10",
    "cloudscraper-promise": "^1.0.0",
    "colors": "^1.4.0",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "prettier": "^2.5.0",
    "progress": "^2.0.3"
  },
  "eslintIgnore": [
    "node_modules"
  ],
  "eslintConfig": {
    "parser": "@babel/eslint-parser",
    "plugins": [
      "no-null"
    ],
    "parserOptions": {
      "sourceType": "script",
      "babelOptions": {
        "plugins": [
          "@babel/plugin-proposal-class-properties",
          "@babel/plugin-proposal-private-methods"
        ]
      }
    },
    "extends": [
      "airbnb-base",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "global-require": "off",
      "no-console": "off",
      "class-methods-use-this": "off",
      "no-param-reassign": [
        "error",
        {
          "props": false
        }
      ],
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": [
            "test/**",
            "build/**"
          ]
        }
      ],
      "no-null/no-null": 2,
      "max-classes-per-file": [
        "error",
        {
          "ignoreExpressions": true
        }
      ],
      "no-await-in-loop": "off"
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-private-methods"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "quoteProps": "as-needed",
    "trailingComma": "es5",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 120
  },
  "mocha": {
    "exit": true,
    "spec": "test/**/*.spec.js",
    "timeout": 10000
  },
  "nyc": {
    "exclude": [
      "test/**",
      "build/**"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "skip-full": true
  },
  "engines": {
    "node": ">=14.18.2"
  },
  "engineStrict": true,
  "clean-package": {
    "remove": [
      "mocha",
      "devDependencies",
      "eslintIgnore",
      "eslintConfig",
      "scripts",
      "clean-package",
      "nyc",
      "babel",
      "prettier"
    ]
  }
}
